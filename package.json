{
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/babblebot-server/server.git"
  },
  "bugs" : {
    "url" : "https://github.com/babblebot-server/server/issues"
  },
  "homepage" : "https://github.com/babblebot-server/server#readme",
  "devDependencies" : {
    "@semantic-release/changelog" : "^5.0.1",
    "@semantic-release/commit-analyzer" : "^8.0.1",
    "@semantic-release/exec" : "^5.0.0",
    "@semantic-release/git" : "^9.0.0",
    "@semantic-release/github" : "^7.2.0",
    "@semantic-release/release-notes-generator" : "^9.0.1",
    "commitlint" : "^11.0.0",
    "conventional-changelog-conventionalcommits" : "^4.5.0",
    "cz-conventional-changelog" : "^3.3.0",
    "gradle-semantic-release-plugin" : "^1.4.13",
    "husky" : "^8.0.3",
    "lint-staged" : "^10.5.3",
    "prettier" : "^2.2.1",
    "remark-cli" : "^9.0.0",
    "remark-preset-lint-recommended" : "^5.0.0",
    "semantic-release" : "^17.3.0"
  },
  "scripts" : {
    "release" : "semantic-release",
    "prepare" : "husky install"
  },
  "remarkConfig" : {
    "plugins" : [
      "remark-preset-lint-recommended"
    ]
  },
  "release" : {
    "branches" : [
      "dev",
      {
        "name" : "next",
        "channel" : "channel-next",
        "prerelease" : "rc"
      },
      {
        "name" : "2.x.x",
        "range" : "2.x.x",
        "channel" : "2.x.x"
      }
    ],
    "plugins" : [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset" : "angular",
          "releaseRules" : [
            {
              "type" : "refactor",
              "release" : "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "gradle-semantic-release-plugin",
      [
        "@semantic-release/exec",
        {
          "prepareCmd" : "bash -c \"rm -rf ./app/build/distributions && ./gradlew assembleDist\""
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd" : "bash -c \"./scripts/update-since-tags.sh\""
        }
      ],
      [
        "@semantic-release/github"
      ],
      [
        "@semantic-release/git",
        {
          "assets" : [
            "./**/*.java",
            "gradle.properties",
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "config" : {
    "commitizen" : {
      "path" : "./node_modules/cz-conventional-changelog"
    }
  }
}
